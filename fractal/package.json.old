{
  "name": "fractal",
  "version": "1.0.0",
  "description": "Note: Please ensure you have installed nodejs",
  "scripts": {
    "start": "concurrently \"node convert-images.js --watch\"  \"parcel src/*.html --no-cache\"",
    "build": "parcel build src/*.html --dist-dir ./build && node convert-images.js"
  },

  
  "targets": {
    "default": {
      "distDir": "build",
      "publicUrl": "./",
      "includeNodeModules": false,
      "sourceMap": false,
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      },
      "outputFormat": "esmodule",
      "optimize": true
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@parcel/plugin": "^2.8.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "parcel": "^2.12.0",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.41",
    "posthtml": "^0.16.6",
    "posthtml-webp": "^2.2.0",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.10"
  },
  "keywords": []
}
